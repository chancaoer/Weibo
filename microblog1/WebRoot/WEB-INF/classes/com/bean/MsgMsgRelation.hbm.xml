<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bean">

	<class name="MsgMsgRelation" table="t_msg_msg_relation" lazy="false">

		<id name="id">
			<generator class="native" />
		</id>
		<property name="referce_id" column="referce_id" />
		<property name="referce_msg_id" column="referce_msg_id" />
		<property name="referced_id" column="referced_id" />
		<property name="referenced_msg_id" column="referenced_msg_id" />
		<property name="w_type" column="w_type" />
		<property name="r_time" column="r_time" />



	</class>

</hibernate-mapping>